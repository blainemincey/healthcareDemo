{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          ">= 0.5%",
          "last 1 version",
          "ie >= 10",
          "not dead"
        ],
        "node": "6"
      },
      "useBuiltins": "usage",
    }]
  ],
  "plugins": [
    "transform-regenerator",
    "transform-async-to-generator",
    "babel-plugin-add-module-exports",
    "version-transform",
    ["babel-plugin-transform-builtin-extend", { globals: ["Error"] }]
  ]
}
